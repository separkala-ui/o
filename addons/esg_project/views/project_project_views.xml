<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="esg_initiatives_server_action" model="ir.actions.server">
        <field name="name">Initiatives Server Action</field>
        <field name="model_id" ref="project.model_project_project"/>
        <field name="path">esg-project-initiatives</field>
        <field name="state">code</field>
        <field name="code">action = model._view_esg_project_tasks()</field>
    </record>

    <record id="action_load_demo_data" model="ir.actions.server">
        <field name="name">Load ESG Project demo data</field>
        <field name="model_id" ref="project.model_project_project"/>
        <field name="state">code</field>
        <field name="code">action = model._load_demo_data()</field>
    </record>
</odoo>

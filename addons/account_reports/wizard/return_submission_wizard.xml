<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="return_submission_wizard_form" model="ir.ui.view">
        <field name="name">account.return.submission.wizard.form</field>
        <field name="model">account.return.submission.wizard</field>
        <field name="arch" type="xml">
            <form string="Submit">
                <sheet>
                    <field name="instructions" nolabel="1" readonly="1"/>
                </sheet>
                <footer>
                    <button string='Mark as Submitted' name="action_proceed_with_submission" type="object" class="btn-primary" data-hotkey="q"/>
                    <button string="Discard" class="btn-secondary" special="cancel" data-hotkey="x"/>
                </footer>
           </form>
        </field>
    </record>
</odoo>

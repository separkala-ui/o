<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="lt_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_lt.lt_tax_report"/>
            <field name="country_id" ref="base.lt" />
            <field name="default_deadline_days_delay">25</field>
            <field name="states_workflow">generic_state_review_submit</field>
        </record>

        <record id="lt_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="l10n_lt_reports.lithuanian_ec_sales_report"/>
            <field name="country_id" ref="base.lt" />
            <field name="auto_generate" eval="False"/>
            <field name="default_deadline_days_delay">25</field>
        </record>
    </data>
</odoo>

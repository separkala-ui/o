<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ie_tax_return_type" model="account.return.type">
            <field name="name">Tax</field>
            <field name="report_id" ref="l10n_ie.l10n_ie_tr"/>
            <field name="country_id" ref="base.ie" />
            <field name="default_deadline_days_delay">19</field>
        </record>

        <!-- EC sales list -->
        <record id="ie_ec_sales_list_return_type" model="account.return.type">
            <field name="name">EC Sales List</field>
            <field name="report_id" ref="account_reports.generic_ec_sales_report"/>
            <field name="default_deadline_periodicity">monthly</field>
            <field name="auto_generate" eval="False"/>
            <field name="country_id" ref="base.ie"/>
        </record>
    </data>
</odoo>

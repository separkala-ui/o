<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">
    <record id="hr_work_entry.work_entry_type_attendance" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">G</field>
    </record>
    <record id="hr_work_entry.work_entry_type_overtime" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
    </record>

    <!-- Time Off Entries -->
    <record id="hr_work_entry.l10n_au_work_entry_type_paid_time_off" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">O</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_long_service_leave" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">O</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_personal_leave" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">O</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_other" model="hr.work.entry.type">
        <field name="is_leave" eval="True"/>
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">O</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_parental" model="hr.work.entry.type">
        <field name="is_leave" eval="True"/>
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">P</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_compensation" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">W</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_defence" model="hr.work.entry.type">
        <field name="is_leave" eval="True"/>
        <field name="l10n_au_is_ote" eval="True"/>
        <field name="l10n_au_work_stp_code">A</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_cash_out" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_termination" model="hr.work.entry.type">
        <field name="l10n_au_is_ote" eval="True"/>
    </record>

    <!-- Overtime Entries -->
    <!-- Remove the record in master, switch to hr_work_entry.overtime_work_entry_type -->
    <record id="hr_work_entry.l10n_au_work_entry_type_overtime_regular" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
        <field name="active" eval="False"/>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_overtime_saturday" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_overtime_sunday" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_overtime_pto" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_overtime_saturday_pto" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
    </record>

    <record id="hr_work_entry.l10n_au_work_entry_type_overtime_sunday_pto" model="hr.work.entry.type">
        <field name="l10n_au_work_stp_code">T</field>
    </record>
</data>

</odoo>

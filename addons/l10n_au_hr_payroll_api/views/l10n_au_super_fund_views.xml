<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="l10n_au_super_fund_view_tree" model="ir.ui.view">
        <field name="name">l10n_au.super.fund.view.list</field>
        <field name="model">l10n_au.super.fund</field>
        <field name="inherit_id" ref="l10n_au_hr_payroll.l10n_au_super_fund_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//list" position="attributes">
                <attribute name="decoration-danger">fund_type == 'APRA' and not is_valid</attribute>
            </xpath>
        </field>
    </record>

</odoo>

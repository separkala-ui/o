msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <h2>Congratulations!</h2>\n"
"                You can configure your salary package by clicking on the "
"link below.<br/>\n"
"                <t t-if=\"ctx.get('validity_end')\">\n"
"                    The link will expire on <t t-"
"out=\"ctx.get('validity_end')\"/>.\n"
"                </t>\n"
"                <div style=\"padding: 16px 0px 16px 0px;\">\n"
"                    <a t-att-href=\"'/salary_package/simulation/offer/%s%s' "

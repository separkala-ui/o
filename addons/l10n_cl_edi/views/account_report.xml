<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->
        <record id="account_invoices_cedible" model="ir.actions.report">
            <field name="name">Invoice PDF copy (Chile)</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">l10n_cl_edi.report_invoice_with_cedible</field>
            <field name="domain">[]</field>
            <field name="report_file">l10n_cl_edi.report_invoice_with_cedible</field>
            <field name="print_report_name">object._l10n_cl_get_report_base_filename()</field>
            <field name="attachment"/>
            <field name="binding_model_id" ref="model_account_move"/>
            <field name="binding_type">report</field>
            <field name="group_ids" eval="[(4, ref('account.group_account_invoice')), (4, ref('account.group_account_readonly'))]"/>
        </record>
    </data>
</odoo>

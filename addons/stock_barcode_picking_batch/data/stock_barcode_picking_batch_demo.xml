<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">
    <!-- Create some cluster packages -->
    <record id="stock_picking_batch_package_type" model="stock.package.type">
            <field name="name">Cluster packs</field>
            <field name="sequence_code">CLU</field>
            <field name="package_use">reusable</field>
    </record>

    <record id="stock_picking_batch_package_01" model="stock.package">
        <field name="name">CLUSTER-PACK-1</field>
        <field name="package_type_id" ref="stock_picking_batch_package_type"/>
    </record>
    <record id="stock_picking_batch_package_02" model="stock.package">
        <field name="name">CLUSTER-PACK-2</field>
        <field name="package_type_id" ref="stock_picking_batch_package_type"/>
    </record>
</data>
</odoo>

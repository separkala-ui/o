<?xml version="1.0"?>
<odoo>     
<record id="appointment_answer_view_form" model="ir.ui.view">
    <field name="name">appointment.answer.view.form</field>
    <field name="model">appointment.answer</field>
    <field name="arch" type="xml">
        <form string="Answer">
            <sheet>
                <group>
                    <group>
                        <field name="name"/>
                    </group>
                    <group>
                        <field name="question_id" readonly="1"/>
                    </group>
                </group>
            </sheet>
        </form>
    </field>
</record>
</odoo>

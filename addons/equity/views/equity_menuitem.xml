<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Top menu item -->
    <menuitem
        id="menu_equity"
        groups="equity.group_equity_viewer"
        web_icon="equity,static/description/icon.png"
        sequence="61"
        name="Equity">
        <menuitem id="menu_equity_dashboard" name="Dashboard" action="equity.action_equity_dashboard_res_partner" sequence="10"/>
        <menuitem id="menu_equity_ownership" name="Ownership" sequence="20">
            <menuitem id="menu_equity_ownership_transactions" name="Transactions" action="equity.action_equity_transaction" sequence="10"/>

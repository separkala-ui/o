msgstr "<code>busta paga</code>: oggetto hr.payslip"

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid ""
"<code>result</code>: boolean True if the rule should be calculated, False "
"otherwise"
msgstr ""
"<code>risultato</code>: booleano True se la regola deve essere calcolata, "
"altrimenti False"

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid "<code>result</code>: float, base amount of the rule"
msgstr "<code>risultato</code>: float, importo di base della regola"

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid ""
"<code>result_name</code>: string, name of the line, which defaults to the "
"name field of the salary rule (useful if the name depends or should depend "
"on something computed in the rule)."
msgstr ""
"<code>result_name</code>: stringa, nome della riga, il cui valore "
"predefinito è il campo nome della regola di retribuzione (utile se il nome "
"dipende o dovrebbe dipendere da qualcosa calcolato nella regola)."

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid "<code>result_qty</code>: float, quantity, which defaults to 1."
msgstr ""
"<code>result_qty</code>: float, quantità, per impostazione predefinita "
"uguale a 1."

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid ""
"<code>result_rate</code>: float, rate between -100.0 and 100.0, which "
"defaults to 100.0 (%)."
msgstr ""
"<code>result_rate</code>: float, valore compreso tra -100,0 e 100,0, il cui "
"valore predefinito è 100,0 (%)."

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid ""
"<code>result_rules</code>: dict containing the rules amounts, quantities, "
"rates and totals (previously computed)"
msgstr ""
"<code>result_rules</code>: dizionario che contiene gli importi, le quantità, "
"i tassi e i totali delle regole (calcolati precedentemente)"

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid "<code>total = result * result_rate / 100 * result_qty</code>"
msgstr "<code>totale = result * result_rate / 100 * result_qty</code>"

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid "<code>version</code>: hr.version object"
msgstr "<code>versione</code>: oggetto hr.version"

#. module: hr_payroll
#: model_terms:ir.ui.view,arch_db:hr_payroll.hr_salary_rule_form
msgid "<code>worked_days</code>: dict containing the computed worked days"
msgstr ""
"<code>worked_days</code>: dizionario che contiene i giorni di lavoro "
"calcolati"

